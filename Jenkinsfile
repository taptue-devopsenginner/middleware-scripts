pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello'
                sleep 5
            }
        }
        stage('Building') {
            steps {
                echo 'Build'
                sleep 5
            }
        }
        stage('Testing') {
            steps {
                echo 'Test'
                sleep 5
            }
        }
        stage('Deployment') {
            steps {
                echo 'Deployment'
                sleep 5
            }
        }
        stage('production') {
            steps {
                echo 'you will be okay'
                sleep 5
                
            }
        }
    }
}