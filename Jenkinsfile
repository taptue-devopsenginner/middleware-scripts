pipeline {
    agent any

    stages {
        stage('pull-from-git') {
            steps {
                echo 'Hello'
                sleep 5
            }
        }
        stage('Building') {
            steps {
                echo 'Build'
                sleep 5
            }
        }
        stage('Testing') {
            steps {
                echo 'Test'
                sleep 5
            }
        }
        stage('artifact-create') {
            steps {
                echo 'Deployment'
                sleep 5
            }
        }
        stage('upload-artifact') {
            steps {
                echo 'i love you so much'
                sleep 5
                
            }
        }
    }
}
